{
"manifest_version": 2,
"name": "Qualitativly",
"version": "1",

"permissions":["pageCapture", "activeTab", "tabs", "storage", "<all_urls>", "notifications"],

"background":{
    "scripts": ["background.js"],
    "persistent":false
}, 
"browser_action": {
    "default_popup": "index.html",
  "default_icons": "QLogo128.png"
  },
  "icons": { "16": "QLogo16.png",
    "32": "QLogo48.png",
    "48": "QLogo48.png",
    "128": "QLogo128.png" },

  "content_scripts": [
    {
        "matches":["file:///popup.html*", "http://*/*", "https://*/*"],
        "js": ["popup.js", "contentscript.js"]
    }],
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  

}